   "outputs": [],
   "source": [
    "prikazi_verjetnosti [(\"sonce\", 0.5); (\"dež\", 0.1); (\"oblačno\", 0.2); (\"sneg\", 0.1)]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
