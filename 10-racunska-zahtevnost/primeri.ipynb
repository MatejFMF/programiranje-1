   "source": [
    "let nakljucno_celo a b =\n",
    "  a + Random.int (b + 1 - a)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "vscode": {
     "languageId": "ocaml"
    }
   },
   "outputs": [],
   "source": [
    "let premesaj tabela =\n",
    "  let n = Array.length tabela in\n",
    "  for i = 0 to n - 2 do\n",
    "    let j = nakljucno_celo (i + 1) (n - 1) in\n",
    "    zamenjaj tabela i j\n",
    "  done"
   ]
